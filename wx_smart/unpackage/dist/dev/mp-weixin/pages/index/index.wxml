<view class="index-content"><view class="index-header"><view class="icon_header"><view data-event-opts="{{[['tap',[['toSearchIndex',['$event']]]]]}}" class="index-search" bindtap="__e"><view class="icon_search"><text class="iconfont icon-search"></text><text>请输入您需要搜索的商品名称</text></view></view><view data-event-opts="{{[['tap',[['toZujiIndex',['$event']]]]]}}" class="icon_suji" bindtap="__e"><text class="iconfont icon-zuji"></text></view></view></view><view class="index-banner"><block wx:if="{{banner.length>0}}"><view class="swiper"><swiper class="swiper-container" autoplay="{{true}}" interval="{{4000}}" circular="{{true}}" indicator-dots="{{true}}" indicator-active-color="#FC3F78" indicator-color="#cccccc"><block wx:for="{{banner}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><swiper-item class="swiper-wrapper"><image src="{{item.image}}" mode="widthFix"></image></swiper-item></block></block></swiper></view></block></view><block wx:if="{{navlist.length>0}}"><view class="index-navlist s-grids has-bg-white has-pdtb-10"><block wx:for="{{navlist}}" wx:for-item="nav" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toNavList',['$0','$1'],[[['navlist','',index,'url']],[['navlist','',index,'title']]]]]]]}}" class="is-col-1-5 is-center" bindtap="__e"><view class="has-pdtb-5"><image src="{{nav.image}}" mode="widthFix"></image><view class="is-size-14">{{nav['title']}}</view></view></view></block></view></block><block wx:if="{{juhuasuanlist.length>0}}"><view class="home_ant_juhuasuan has-bg-white"><view class="juhuasuan-tab s-row"><text class="fl-jutext">聚拼团</text><text class="fr-jutext">查看更多拼团</text></view><view class="s-row juhuasuan-list"><block wx:for="{{juhuasuanlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toGoodsInfo',['$0'],[[['juhuasuanlist','',index,'itemid']]]]]]]}}" class="juhuasuan-list-goods" bindtap="__e"><view class="image"><image src="{{item.picture}}" mode="widthFix"></image><view class="name"><text class="pinname">{{"￥"+item.price}}</text><text class="price"></text></view></view></view></block></view></view></block><view class="index-coupon has-bg-white has-pd-10"><view class="coupon-tab s-row"><text class="fl-jutext">领券直播</text><text class="fr-jutext">专享超值优惠券</text></view><block wx:if="{{couponlist.length>0}}"><view class="goods-list"><block wx:for="{{couponlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toGoodsInfo',['$0'],[[['couponlist','',index,'itemid']]]]]]]}}" class="coupon-page s-row" bindtap="__e"><view class="image"><image src="{{item.picture}}"></image></view><view class="content"><view class="title">{{item.title}}</view><view class="num s-row"><text class="tmprice">{{'天猫价 ¥'+item.price}}</text><text class="volume">{{"已售"+item.volume+"件"}}</text></view><view class="money s-row"><text class="coupon-price">券后价<text>{{"¥"+item.yprice}}</text></text><text class="quan"><view class="_i"></view>{{item.coupon+"元券"}}<view class="_i"></view></text></view></view></view></block></view></block></view><block wx:if="{{couponlist.length>0}}"><view class="s-col is-col-24"><load-more vue-id="1" loadingType="{{loadingType}}" contentText="{{contentText}}" bind:__l="__l"></load-more></view></block></view>